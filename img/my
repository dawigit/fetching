#!/usr/bin/env bash
colors=("0;0;0" "32;43;64" "86;105;115" "121;121;121" "223;47;47" "65;97;251" "124;139;166" "97;211;227" "191;191;191" "255;186;162" "145;133;204" "217;255;255" "255;255;255" )

for c in ${!colors[@]}; do
 cl=${colors[$c]}
 printf -v c$c %b "[38;2;${cl}m"
done
bd=$'\e[1m'
rt=$'\e[0m'
iv=$'\e[7m'
